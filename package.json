    "posttest-coverage": "rimraf .nyc_output",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "lint-fix": "npm run lint -- --fix",
    "check": "run-s build test lint",
    "postcheck": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
